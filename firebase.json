{
  "hosting": [
    {
      "site": "cognite-stlite-standalone-staging",
      "public": "/tmp/website/",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "site": "cognite-stlite-standalone-production",
      "public": "/tmp/website/",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ]
}
